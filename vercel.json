{
    "rewrites": [
        {
            "source": "/assets/data/content.json",
            "destination": "/api/content"
        }
    ],
    "headers": [
        {
            "source": "/assets/data/content.json",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "no-store, no-cache, must-revalidate, proxy-revalidate"
                },
                {
                    "key": "Access-Control-Allow-Origin",
                    "value": "https://huyhoangkimthoa-chinese-style.vercel.app"
                }
            ]
        }
    ]
}